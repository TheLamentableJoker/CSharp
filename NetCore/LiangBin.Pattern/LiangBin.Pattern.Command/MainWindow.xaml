<Window x:Class="LiangBin.Pattern.Command.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LiangBin.Pattern.Command"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Click="Button_Click" Content="Button" HorizontalAlignment="Left" Margin="78,10,0,0" VerticalAlignment="Top"/>
        <RichTextBox Name="currentRichTextBox" Margin="34,54,58,28">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                    <InlineUIContainer>
                        <TextBlock Text="TextBlock" TextWrapping="Wrap"/>
                    </InlineUIContainer>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
